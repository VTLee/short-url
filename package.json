{
  "name": "short-url",
  "version": "1.0.0",
  "description": "short-url",
  "main": "index.js",
  "scripts": {
    "test": "cd spec && mocha --require ts-node/register *.ts",
    "build": "rm -rf dist/; tsc --outDir dist/ && cp -R src/config.yaml src/version.json src/schemas dist/ && cp -R src/scripts/** dist/scripts",
    "bundle": "rm -f dist.zip; zip -rq dist.zip node_modules/* && cd dist/ && zip -ur ../dist.zip *",
    "dev": "npm run populate-test && npm start",
    "populate-test": "cd dist && node scripts/populateData.js",
    "start": "export NODE_ENV=dev && cd dist && ../node_modules/.bin/configurapi-runner-self -p 8001",
    "apiunit-cicd": "cd apiunit && ts-node ../node_modules/.bin/apiunit -i requests -d responses -p parameters_cicd.json"
  },
  "files": [
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VTLee/short-url.git"
  },
  "author": "Lee Doughty",
  "license": "All rights reserved",
  "bugs": {
    "url": "https://github.com/VTLee/short-url/issues"
  },
  "homepage": "https://github.com/VTLee/short-url#readme",
  "dependencies": {
    "base32": "0.0.6",
    "configurapi": "^1.4.7",
    "configurapi-handler-fs": "^1.0.1",
    "configurapi-handler-http": "^1.3.4",
    "configurapi-handler-json": "^1.3.1",
    "configurapi-handler-logging": "^1.3.6",
    "configurapi-runner-lambda-api": "^1.0.10",
    "configurapi-runner-self": "^1.4.0",
    "nodenamo": "^0.8.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.0",
    "apiunit": "^2.1.9",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "sinon": "^7.3.2",
    "ts-node": "^8.0.3",
    "typemoq": "^2.1.0",
    "typescript": "^3.4.1"
  }
}
